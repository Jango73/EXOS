{
    "logs": [
        "log/kernel.log"
    ],
    "scriptsDir": "./scripts",
    "keyBindings": {
        "C-e": { "label": "Setup Dependencies", "script": "1-1-setup-deps.sh" },
        "C-f": { "label": "Format Code", "script": "3-1-format-all-code.sh" },
        "C-h": { "label": "Build help", "script": "3-2-generate-kernel-docs.sh" },
        "C-w": { "label": "Start server", "script": "net/start-server.sh" },

        "i386-----------------": "",

        "f1": { "label": "Clean Build", "script": "scripts/build --arch i386 --fs ext2 --release --clean" },
        "f2": { "label": "Build", "script": "scripts/build --arch i386 --fs ext2 --release" },

        "f3": { "label": "Clean Build Debug", "script": "scripts/build --arch i386 --fs ext2 --debug --clean" },
        "f4": { "label": "Build Debug", "script": "scripts/build --arch i386 --fs ext2 --debug" },

        "f5": { "label": "Start QEMU", "script": "scripts/run --arch i386" },
        "f6": { "label": "Start QEMU + GDB", "script": "scripts/run --arch i386 --gdb" },

        "x86-64---------------": "",

        "f7": { "label": "Clean Build", "script": "scripts/build --arch x86-64 --fs ext2 --release --clean" },
        "f8": { "label": "Build", "script": "scripts/build --arch x86-64 --fs ext2 --release" },

        "f9": { "label": "Clean Build Debug", "script": "scripts/build --arch x86-64 --fs ext2 --debug --clean" },
        "f10": { "label": "Build Debug", "script": "scripts/build --arch x86-64 --fs ext2 --debug" },

        "f11": { "label": "Start QEMU", "script": "scripts/run --arch x86-64" },
        "f12": { "label": "Start QEMU + GDB", "script": "scripts/run --arch x86-64 --gdb" },

        "3--------------------": "",

        "p": { "label": "Git Pull", "script": "git pull" },
        "C-p": { "label": "Git Push", "script": "git push" },
        "s": { "label": "Git Status", "script": "git status" },
        "C-s": { "label": "Git Diff Staged", "script": "git diff --staged" }
    },
    "settings": {
        "enableCommandHistory": true,
        "persistLogs": true,
        "notifyOnExit": true,
        "renderThrottleMs": 50,
        "maxLogLines": 1000,
        "sidebarMinWidth" : 25
    },
    "events": {
        "onDashboardStart": [
            {
                "action": "killProcess",
                "parameters": [
                    "qemu-system-i386"
                ]
            },
            {
                "action": "killProcess",
                "parameters": [
                    "qemu-system-x86_64"
                ]
            }
        ],
        "beforeStartProcess": []
    }
}
