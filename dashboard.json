{
    "logs": [
        "log/kernel.log"
    ],
    "scriptsDir": "./scripts",
    "keyBindings": {
        "C-e": { "label": "Setup Dependencies", "script": "1-setup-deps.sh" },
        "C-f": { "label": "Format Code", "script": "3-format-all-code.sh" },

        "i386-----------------": "",

        "f1": { "label": "Clean Build", "script": "i386/4-1-clean-build-release-ext2.sh" },
        "f2": { "label": "Build", "script": "i386/4-4-build-release-ext2.sh" },

        "f3": { "label": "Clean Build Debug", "script": "i386/4-2-clean-build-debug-ext2.sh" },
        "f4": { "label": "Build Debug", "script": "i386/4-5-build-debug-ext2.sh" },

        "f5": { "label": "Start QEMU", "script": "i386/5-1-start-qemu-ioapic-sata-e1000.sh" },
        "f6": { "label": "Start QEMU + GDB", "script": "i386/5-2-debug-qemu-ioapic-sata-e1000.sh" },

        "x86-64---------------": "",

        "f7": { "label": "Clean Build", "script": "x86-64/4-1-clean-build-release-ext2.sh" },
        "f8": { "label": "Build", "script": "x86-64/4-4-build-release-ext2.sh" },

        "f9": { "label": "Clean Build Debug", "script": "x86-64/4-2-clean-build-debug-ext2.sh" },
        "f10": { "label": "Build Debug", "script": "x86-64/4-5-build-debug-ext2.sh" },

        "f11": { "label": "Start QEMU", "script": "x86-64/5-1-start-qemu-ioapic-sata-e1000.sh" },
        "f12": { "label": "Start QEMU + GDB", "script": "x86-64/5-2-debug-qemu-ioapic-sata-e1000.sh" },

        "3--------------------": "",

        "p": { "label": "Git Pull", "script": "git pull" },
        "C-p": { "label": "Git Push", "script": "git push" },
        "s": { "label": "Git Status", "script": "git status" },
        "C-s": { "label": "Git Diff Staged", "script": "git diff --staged" }
    },
    "settings": {
        "enableCommandHistory": true,
        "persistLogs": true,
        "notifyOnExit": true,
        "renderThrottleMs": 100,
        "maxLogLines": 1000
    },
    "events": {
        "onDashboardStart": [
            {
                "action": "killProcess",
                "parameters": [
                    "qemu-system-i386"
                ]
            },
            {
                "action": "killProcess",
                "parameters": [
                    "qemu-system-x86_64"
                ]
            }
        ],
        "beforeStartProcess": []
    }
}
